<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations
    xmlns="http://agavi.org/agavi/config/parts/validators/1.0"
    xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0"
    parent="%core.module_dir%/%%MODULE_NAME%%/config/validators.xml">
    <ae:configuration>
        <validators>
            <validator class="ListStateValidator" name="invalid_list_state" required="true" />
            <validator class="string" name="invalid_reference_module" required="false">
                <arguments>
                    <argument>referenceModule</argument>
                </arguments>
            </validator>
            <validator class="string" name="invalid_reference_field" required="false">
                <arguments>
                    <argument>referenceField</argument>
                </arguments>
            </validator>
            <validator class="string" name="invalid_export_format" required="false">
                <arguments>
                    <argument>export_format</argument>
                </arguments>
            </validator>
        </validators>
    </ae:configuration>
</ae:configurations>
